<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="(%d{ISO8601}) %t [%C{1}:%L] %p -> %m %n"/>
    </Console>
  </Appenders>
  <Loggers>
  
  <Logger name="br.com.patiolegal" level="debug"/>
  
  <Logger name="org.hibernate.SQL" level="debug"/>
  
    <Root level="error">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>